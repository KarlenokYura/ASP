@using Lab_3.Models
@{
    Layout = null;
    List<Telephone> telephones = ViewBag.getAll;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>Hello World!</h1>
        <h2>Get All</h2>
        @foreach (Telephone telephone in telephones)
        {
            <p>[@telephone.id] @telephone.surname - @telephone.number</p>
        }
        <h2>@Html.ActionLink("Add", "Add")</h2>
        <h2>@Html.ActionLink("Update", "Update")</h2>
        <h2>@Html.ActionLink("Delete", "Delete")</h2>
    </div>
</body>
</html>
